<% if user_signed_in? %>
  <div class="card m-5" style="width: 13rem; height: 9rem">
    <div class="card-body">
      <div class="d-flex" style= "width:100%">
        <h1 style= "width:100%"><%= @count %></h1>
        <div><%= image_tag "employees.png"%></div>
      </div>
      <h5 class="card-text mt-3" style= "text-align:center">Employees</h5>
    </div>
  </div>
 
<% else %>
  <p>Register to get started.</p>
<% end %>
